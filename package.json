{
	"author": {
		"email": "matt@polymermallard.com",
		"name": "Matt Kenefick"
	},
	"bugs": {
		"url": "https://github.com/mattkenefick/vscode-gpt-template/issues"
	},
	"categories": [
		"Other"
	],
	"contributes": {
		"configuration": {
			"title": "GPT Template",
			"properties": {
				"gpt-template.token": {
					"type": "string",
					"default": "",
					"description": "OpenAI token for API access"
				}
			}
		},
		"commands": [
			{
				"command": "gpt-template.useAsTemplate",
				"title": "Generate using this as template"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "gpt-template.useAsTemplate"
				}
			]
		}
	},
	"description": "Creates a new file using an existing one as a template.",
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"@vscode/vsce": "^2.19.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"ts-loader": "^9.2.2",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"displayName": "GPT Template",
	"engines": {
		"vscode": "^1.75.0"
	},
	"galleryBanner": {
		"color": "#d9d9d9",
		"theme": "light"
	},
	"homepage": "https://github.com/mattkenefick/vscode-gpt-template",
	"icon": "assets/logo.png",
	"keywords": [
		"ai",
		"chatgpt",
		"openai",
		"template",
		"gpt"
	],
	"license": "MIT",
	"main": "./dist/extension.js",
	"name": "gpt-template",
	"publisher": "PolymerMallard",
	"repository": {
		"type": "git",
		"url": "https://github.com/mattkenefick/vscode-gpt-template.git"
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"version": "0.1.0",
	"dependencies": {
		"axios": "^1.5.1"
	}
}
